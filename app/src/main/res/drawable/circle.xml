<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">


    <item>
        <shape
            android:shape="oval">
            <solid android:color="#FFFFFF"/>
            <stroke
                android:width="1.5dp"
                android:color="#3B5CD3"/>
            <corners android:radius="19dp"/>
        </shape>
    </item>

    <item android:drawable="@drawable/menu_dots"
        android:left="40dp"
    android:right="40dp"
        android:top="40dp"
        android:bottom="40dp"
    android:gravity="center"/>

</layer-list>

